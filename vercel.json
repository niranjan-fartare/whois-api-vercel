{
  "version": 2,
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node"
    }
  ],
  "headers": [
    {
      "source": "/api/whois",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://hostingchecker.co" 
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type"
        }
      ]
    }
  ]
}
